{
  "extends": ["tslint:recommended","tslint:react"],
  "rules": {
    "max-line-length": {
      "options": [
        120
      ]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "ordered-imports": false,
    "no-console":true,
    "no-empty":true,
    "no-unused-variable":true
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        120
      ]
    }
  }
}